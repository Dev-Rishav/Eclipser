# Staging Environment Variables Template
# Copy this file to .env.staging and fill in your staging values

VITE_API_URL=https://staging-api.yourdomain.com
VITE_SOCKET_URL=https://staging-api.yourdomain.com
VITE_CONTEST_API_URL=https://staging-contest-api.yourdomain.com
VITE_ENVIRONMENT=staging

# Cloudinary Configuration (use staging values)
VITE_CLOUDINARY_CLOUD_NAME=your_staging_cloud_name
VITE_CLOUDINARY_API_KEY=your_staging_api_key
VITE_CLOUDINARY_API_SECRET=your_staging_api_secret
